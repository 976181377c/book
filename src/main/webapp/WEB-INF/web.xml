<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>com.javaweb.index</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>shop</servlet-name>
    <servlet-class>com.javaweb.shop</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Buy</servlet-name>
    <servlet-class>com.javaweb.Buy</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>select</servlet-name>
    <servlet-class>com.javaweb.select</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.javaweb.login</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>shop</servlet-name>
    <url-pattern>/shop</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Buy</servlet-name>
    <url-pattern>/Buy</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>select</servlet-name>
    <url-pattern>/select</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>